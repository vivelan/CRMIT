            "query": "q = load \"Cost_of_Production_R_Levels\";\n\n-- blanks the content of the table if the FMN selected is invalid (e.g. no SW present)\nq = filter q by category_1 in {{cell(Generate_error_messa_1.result, 0, \"errorMessageFlag\").asString()}};\n\n-- filter out null categories\nq = filter q by 'category_1' is not null;\n\n-- get starting energy and gast cost\nstartingEnergyCost =  filter q by \n    {{column(Yearly_Cost_Point_1.selection, [\"price_point_year\"]).asEquality(\"price_point_year\")}} && \n    {{column(Monthly_Cost_Point_1.selection, [\"price_point_month\"]).asEquality(\"price_point_month\")}} && \n    {{cell(Get_all_FMN_numbers_1.selection, 0, \"material_no\").asEquality(\"material_no\")}} && (category_3 == \"Delta Gas zu Grundplan\" || \n    category_3 == \"Delta Strom zu Grundplan\");\n\nstartingEnergyCost = foreach startingEnergyCost generate \n    'category_1_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_1', \n    'category_2_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_2', \n    'category_3_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_3', \n    'external_cost' as 'external_cost', \n    'internal_cost' as 'internal_cost';\n\n-- get final energy and gast cost\nfinalEnergyCost =  filter q by \n    {{column(Yearly_Cost_Point_2.selection, [\"price_point_year\"]).asEquality(\"price_point_year\")}} && \n    {{column(Monthly_Cost_Point_2.selection, [\"price_point_month\"]).asEquality(\"price_point_month\")}} && \n    {{cell(Get_all_FMN_numbers_1.selection, 0, \"material_no\").asEquality(\"material_no\")}} && \n    (category_3 == \"Delta Gas zu Grundplan\" || category_3 == \"Delta Strom zu Grundplan\");\n\nfinalEnergyCost = foreach finalEnergyCost generate \n    'category_1_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_1', \n    'category_2_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_2', \n    'category_3_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_3', \n    'external_cost' as 'external_cost', \n    'internal_cost' as 'internal_cost';\n\n-- get starting cost\nstartingCost = filter q by \n    {{column(Yearly_Cost_Point_1.selection, [\"price_point_year\"]).asEquality(\"price_point_year\")}} && \n    {{column(Monthly_Cost_Point_1.selection, [\"price_point_month\"]).asEquality(\"price_point_month\")}} && \n    {{coalesce(cell(Get_SW_ID_1.result, 0, \"SW_ID\"), \"0\").asEquality(\"material_no\")}} && \n    'category_3' != \"{{cell(Include_Eigenschrott_1.selection, 0, \"Value\").asString()}}\";\n\nstartingCost = foreach startingCost generate \n    'category_1_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_1', \n    'category_2_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_2', \n    'category_3_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_3', \n    'external_cost' * {{column(Get_HZV_Quantity_1.result, [\"HZV_Quantity\"]).asObject()}} * {{coalesce(column(Get_SW_Quantity_1.result, [\"SW_Quantity\"]), 0).asObject()}} as 'external_cost', \n    'internal_cost' * {{column(Get_HZV_Quantity_1.result, [\"HZV_Quantity\"]).asObject()}} * {{coalesce(column(Get_SW_Quantity_1.result, [\"SW_Quantity\"]), 0).asObject()}} as 'internal_cost';\n\n-- get final cost\nfinalCost = filter q by\n    {{column(Yearly_Cost_Point_2.selection, [\"price_point_year\"]).asEquality(\"price_point_year\")}} && \n    {{column(Monthly_Cost_Point_2.selection, [\"price_point_month\"]).asEquality(\"price_point_month\")}} && \n    {{coalesce(cell(Get_SW_ID_1.result, 0, \"SW_ID\"), \"0\").asEquality(\"material_no\")}} && \n    'category_3' != \"{{cell(Include_Eigenschrott_1.selection, 0, \"Value\").asString()}}\";\n\nfinalCost = foreach finalCost generate \n    'category_1_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_1', \n    'category_2_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_2', \n    'category_3_{{cell(Get_User_Locale_1.result, 0, \"LanguageLocaleKey\").asString()}}' as 'category_3', \n    'external_cost' * {{column(Get_HZV_Quantity_1.result, [\"HZV_Quantity\"]).asObject()}} * {{coalesce(column(Get_SW_Quantity_1.result, [\"SW_Quantity\"]), 0).asObject()}} as 'external_cost', \n    'internal_cost' * {{column(Get_HZV_Quantity_1.result, [\"HZV_Quantity\"]).asObject()}} * {{coalesce(column(Get_SW_Quantity_1.result, [\"SW_Quantity\"]), 0).asObject()}} as 'internal_cost';\n\n-- cogroup starting cost and final cost streams\nstartingCost = union startingCost, startingEnergyCost;\nfinalCost = union finalCost, finalEnergyCost;\nresult = cogroup startingCost by {{row(R_Level_selector_2.selection, [], [\"Value\"]).asObject()}}, finalCost by {{row(R_Level_selector_2.selection, [], [\"Value\"]).asObject()}};\n\n-- generate the result into the table\nresult = foreach result generate \n    {{cell(R_Level_selector_2.selection, 0, \"Generate\").asString()}}, \n    sum(startingCost.{{cell(Internal_or_External_1.selection, 0, \"Value\").asString()}}) as '{{cell(Get_Flat_Translation_1.result, 0, 'Field_Time_Point_1').asString()}}', \n    sum(finalCost.{{cell(Internal_or_External_1.selection, 0, \"Value\").asString()}}) as '{{cell(Get_Flat_Translation_1.result, 0, 'Field_Time_Point_2').asString()}}', \n    sum(finalCost.{{cell(Internal_or_External_1.selection, 0, \"Value\").asString()}}) - sum(startingCost.{{cell(Internal_or_External_1.selection, 0, \"Value\").asString()}}) as '{{cell(Get_Flat_Translation_1.result, 0, \"Field_Difference\").asString()}}', \n    coalesce((sum(finalCost.{{cell(Internal_or_External_1.selection, 0, \"Value\").asString()}}) - sum(startingCost.{{cell(Internal_or_External_1.selection, 0, \"Value\").asString()}})) / sum(startingCost.{{cell(Internal_or_External_1.selection, 0, \"Value\").asString()}}), 0) as '{{cell(Get_Flat_Translation_1.result, 0, \"Field_Percentage_Difference\").asString()}}',\n    (sum(finalCost.{{cell(Internal_or_External_1.selection, 0, \"Value\").asString()}}) - sum(startingCost.{{cell(Internal_or_External_1.selection, 0, \"Value\").asString()}})) / ({{coalesce(cell(Calculate_Total_Cost_1.result, 1, cell(Internal_or_External_1.selection, 0, \"Value\")), 0).asObject()}} - {{coalesce(cell(Calculate_Total_Cost_1.result, 0, cell(Internal_or_External_1.selection, 0, \"Value\")), 0).asObject()}}) as '{{cell(Get_Flat_Translation_1.result, 0, \"Field_Contribution_to_Cost_Change\").asString()}}';\n\n-- filter out energy and gas and eigenschrott depending on selector value\nresult = filter result by 'R-Level 1' != \"{{cell(Include_Energy_and_G_1.selection, 0, \"Value\").asString()}}\";\n\n-- sort on Contribution to Cost Change\nresult = order result by '{{cell(Get_Flat_Translation_1.result, 0, \"Field_Contribution_to_Cost_Change\").asString()}}' desc; \n\n\n\n\n\n\n",
