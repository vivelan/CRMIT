        "Calculate_Total_Cost_1": {
            "broadcastFacet": true,
            "groups": [],
            "label": "Calculate Total Costs",
            "numbers": [],
            "query": "q = load \"Cost_of_Production_R_Levels\";\n\n-- blanks the content of the table if the FMN selected is invalid (e.g. no SW present)\nq = filter q by category_1 in {{cell(Generate_error_messa_1.result, 0, \"errorMessageFlag\").asString()}};\n\n-- filter by the starting cost point and final cost point to get only the SW rows for those cost points\nq1 = filter q by (({{column(Yearly_Cost_Point_1.selection, [\"price_point_year\"]).asEquality(\"price_point_year\")}} && {{column(Monthly_Cost_Point_1.selection, [\"price_point_month\"]).asEquality(\"price_point_month\")}}) || ({{column(Yearly_Cost_Point_2.selection, [\"price_point_year\"]).asEquality(\"price_point_year\")}} && {{column(Monthly_Cost_Point_2.selection, [\"price_point_month\"]).asEquality(\"price_point_month\")}})) && {{coalesce(cell(Get_SW_ID_1.result, 0, \"SW_ID\"), \"0\").asEquality(\"material_no\")}} && category_3 is not null && 'category_3' != \"{{cell(Include_Eigenschrott_1.selection, 0, \"Value\").asString()}}\";\n\nq1 = group q1 by rollup('price_point_year', 'price_point_month');\nq1 = foreach q1 generate 'price_point_year' as 'price_point_year', 'price_point_month' as 'price_point_month', sum(external_cost)  * {{column(Get_HZV_Quantity_1.result, [\"HZV_Quantity\"]).asObject()}} * {{coalesce(column(Get_SW_Quantity_1.result, [\"SW_Quantity\"]), 0).asObject()}} as 'external_cost', sum(internal_cost) * {{column(Get_HZV_Quantity_1.result, [\"HZV_Quantity\"]).asObject()}} * {{coalesce(column(Get_SW_Quantity_1.result, [\"SW_Quantity\"]), 0).asObject()}} as 'internal_cost';\n\n\n-- filter by R-Level 3 Strom or Gas and by the selected material number\nenergyGas = filter q by ('category_3' == \"Delta Strom zu Grundplan\" || 'category_3' == \"Delta Gas zu Grundplan\") && {{cell(Get_all_FMN_numbers_1.selection, 0, \"material_no\").asEquality(\"material_no\")}};\n--filter by starting cost point and final cost point selection\nenergyGas = filter energyGas by ({{column(Yearly_Cost_Point_1.selection, [\"price_point_year\"]).asEquality(\"price_point_year\")}} && {{column(Monthly_Cost_Point_1.selection, [\"price_point_month\"]).asEquality(\"price_point_month\")}}) || ({{column(Yearly_Cost_Point_2.selection, [\"price_point_year\"]).asEquality(\"price_point_year\")}} && {{column(Monthly_Cost_Point_2.selection, [\"price_point_month\"]).asEquality(\"price_point_month\")}});\n\n\nenergyGas = group energyGas by rollup('price_point_year', 'price_point_month');\nenergyGas = foreach energyGas generate 'price_point_year' as 'price_point_year', 'price_point_month' as 'price_point_month', sum('external_cost') as 'external_cost', sum('internal_cost') as 'internal_cost';\n\n\ntotalCost = cogroup q1 by ('price_point_year', 'price_point_month'), energyGas by ('price_point_year', 'price_point_month');\n\ntotalCost = foreach totalCost generate q1.'price_point_year' as 'price_point_year', q1.'price_point_month' as 'price_point_month', \n(case \nwhen \"{{cell(Include_Energy_and_G_1.selection, 0, \"Include Energy and Gas\").asString()}}\" in [\"Ja\", \"Yes\"] then sum(energyGas.'external_cost') + sum(q1.'external_cost')\nwhen \"{{cell(Include_Energy_and_G_1.selection, 0, \"Include Energy and Gas\").asString()}}\" in [\"Nein\", \"No\"] then sum(q1.'external_cost')\nend\n) as 'external_cost',\n(case \nwhen \"{{cell(Include_Energy_and_G_1.selection, 0, \"Include Energy and Gas\").asString()}}\" in [\"Ja\", \"Yes\"] then sum(energyGas.'internal_cost') + sum(q1.'internal_cost')\nwhen \"{{cell(Include_Energy_and_G_1.selection, 0, \"Include Energy and Gas\").asString()}}\" in [\"Nein\", \"No\"] then sum(q1.'internal_cost')\nend\n) as 'internal_cost';\n\n\n\n\n",
            "receiveFacetSource": {
                "mode": "none",
                "steps": []
            },
            "selectMode": "single",
            "strings": [],
            "type": "saql",
            "useGlobal": false,
            "visualizationParameters": {
                "parameters": {
                    "borderColor": "#e0e5ee",
                    "borderWidth": 1,
                    "cell": {
                        "backgroundColor": "#ffffff",
